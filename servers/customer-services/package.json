  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "test": "jest",
    "test:auth": "jest --testPathPatterns=authentication",
